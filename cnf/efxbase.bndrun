runproperties.efx:  \
	osgi.framework.extensions=org.eclipse.fx.osgi,\
    eclipse.application.launchDefault=true,\
    osgi.compatibility.eagerStart.LazyActivation=true,\
    osgi.compatibility.errorOnFailedStart=false

